{
  "name": "arq-ref-node",
  "version": "1.0.0",
  "description": "A microservice model to use web APIs with Node",
  "main": "index.js",
  "engines": {
    "node": ">=10.13.x <11.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon",
    "dev-debug": "cross-env NODE_PATH=. NODE_ENV=development node --inspect=0.0.0.0:9229 index.js",
    "dev-diagnostic": "cross-env NODE_PATH=. NODE_ENV=development nodejs-dashboard -- node -r nodejs-dashboard index.js",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:features",
    "test:unit": "cross-env NODE_PATH=. NODE_ENV=test mocha --opts test/mocha.opts.unit --reporter mocha-multi-reporters --reporter-options configFile=test/reporter.unit.config.json --exit",
    "test:features": "cross-env NODE_PATH=. NODE_ENV=test mocha --opts test/mocha.opts.features --reporter mocha-multi-reporters --reporter-options configFile=test/reporter.features.config.json --exit",
    "test:integration": "newman run test/integration/API-MS-archref.postman_collection.json -e test/integration/local.postman_environment.json -r cli,json,junit --reporter-json-export test-results/ --reporter-junit-export test-results/",
    "coverage": "cross-env NODE_PATH=. NODE_ENV=test nyc mocha --opts test/mocha.opts.unit --reporter mocha-multi-reporters --exit",
    "lint": "eslint {src,test,config}/**/*.js",
    "console": "cross-env NODE_PATH=. node src/interfaces/console/index.js",
    "config-database-ci": "node scripts/config-database-pipeline.js",
    "seed-data": "node src/infra/database/seeds/index.js",
    "cleanup": "node scripts/cleanup.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Tiago Pache <tiagopache@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@log4js-node/gelf": "^1.0.1",
    "applicationinsights": "^1.3.1",
    "awilix": "^3.0.9",
    "awilix-express": "^0.11.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.16.0",
    "express": "^4.16.4",
    "express-status-monitor": "^1.2.5",
    "helmet": "^3.16.0",
    "http-status": "^1.3.2",
    "log4js": "^4.1.0",
    "method-override": "^3.0.0",
    "mongo-seeding": "^3.1.0",
    "mongoose": "^5.5.3",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "structure": "^1.4.0",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-change": "^2.1.2",
    "chance": "^1.0.18",
    "dirty-chai": "^2.0.1",
    "factory-girl": "^5.0.4",
    "listr": "^0.14.3",
    "mocha": "^6.1.4",
    "mocha-junit-reporter": "^1.22.0",
    "mocha-multi-reporters": "^1.1.7",
    "nodejs-dashboard": "^0.4.3",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "replace-in-file": "^3.4.4",
    "snyk": "^1.153.0",
    "supertest": "^3.4.2"
  }
}
